<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<!-- You can enter your name to save your score. -->
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.example.tetris.GameEndController"
            prefHeight="400.0" prefWidth="600.0">

    <Label layoutX="250.0" layoutY="100.0" text="Game Over" />
    <Label layoutX="250.0" layoutY="150.0" text="Score: 0" />

    <Button onAction="#saveScore" layoutX="250.0" layoutY="250.0" mnemonicParsing="false" text="Save" />
    <Button onAction="#noSaveScore" layoutX="250.0" layoutY="300.0" mnemonicParsing="false" text="Don't save" />
</AnchorPane>
